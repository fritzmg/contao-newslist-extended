{
    "name": "fritzmg/contao-newslist-extended",
    "description": "Contao extension to add a few more features to the news list.",
    "type": "contao-bundle",
    "license": "LGPL-3.0-or-later",
    "homepage": "https://github.com/fritzmg/contao-newslist-extended",
    "authors": [
        {
            "name": "Fritz Michael Gschwantner",
            "email": "fmg@inspiredminds.at",
            "role": "Developer"
        }
    ],
	"support": {
		"issues": "https://github.com/fritzmg/contao-newslist-extended/issues",
		"source": "https://github.com/fritzmg/contao-newslist-extended",
		"forum": "https://community.contao.org/de"
	},
	"funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/fritzmg"
        }
    ],
    "require": {
        "php": ">=7.4",
        "contao/core-bundle": "^4.13 || ^5.0",
        "contao/news-bundle": "^4.13 || ^5.0",
        "doctrine/dbal": "^2.11 || ^3.0",
        "inspiredminds/contao-news-filter-event": "^1.0",
        "symfony/dependency-injection": "^4.4 || ^5.1 || ^6.0",
        "symfony/http-kernel": "^4.4 || ^5.1 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "InspiredMinds\\ContaoNewslistExtended\\": "src/"
        }
    },
    "extra": {
        "contao-manager-plugin": "InspiredMinds\\ContaoNewslistExtended\\ContaoManager\\Plugin"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16 || ^3.0",
        "rector/rector": "^0.14.5"
    },
    "config": {
        "allow-plugins": {
            "contao-components/installer": true,
            "php-http/discovery": false
        }
    }
}
